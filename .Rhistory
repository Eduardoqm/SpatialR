polycent <- function(x){
N <- length(x)  #Number of polygons
Centroids.bb <- data.frame(matrix(NA, N, 2, dimnames = list(NULL, c("long", "lat"))))
for(i in 1:N){
bb <- bbox(x@polygons[[i]]) #Bounding box of polygon
Centroids.bb[i,] <- c(
0.5 * (bb[1,1] + bb[1,2]),
0.5 * (bb[2,1] + bb[2,2]))  #Compute centroids
}
return(Centroids.bb)
}
